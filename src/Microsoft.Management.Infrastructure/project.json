{
    "name": "Microsoft.Management.Infrastructure",
    "version": "1.0.0-*",
    "authors": [ "cbnilrem", "garretts", "andschwa" ],

    "buildOptions": {
        "allowUnsafe": true
    },

    "configurations": {
        "Linux": {
            "buildOptions": {
                "define": [ "_LINUX" ]
            }
        }
    },

    "frameworks": {
        "netstandard1.5": {
            "imports": [ "dnxcore50" ],
            "buildOptions": {
                "warningsAsErrors": true,
                "define": [ "_CORECLR" ]
            },
            "dependencies": {
                "NETStandard.Library": "1.5.0-rc2-24103",
                "System.Security.SecureString": "4.0.0-rc1",
                "System.Runtime.Serialization.Xml": "4.1.1-rc2-24103",
                "System.Threading.ThreadPool": "4.0.10-rc2-24103"
            }
        },
        "net451": {
        }
    }
}
